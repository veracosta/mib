{
    "name": "mib",
    "displayName": "SNMP MIB",
    "description": "Syntx highlighting for SNMP MIB file",
    "version": "0.0.3",
    "publisher": "Veracosta",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mib",
                "aliases": [
                    "MIB",
